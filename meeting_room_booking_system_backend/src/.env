# nodemailer 相关配置
nodemailer_host=smtp.qq.com
nodemailer_port=587
nodemailer_auth_user=xxxx@xx.com
nodemailer_auth_pass=xxxx

# jwt 相关配置
jwt_secret=shu
jwt_access_token_expires_time=30m
jwt_refresh_token_expires_time=7d

# winston 日志配置
winston_log_level=debug
winston_log_dirname=daily-log
winston_log_filename=log-%DATE%.log
winston_log_date_pattern=YYYY-MM-DD
winston_log_max_size=1M

